<!--Header Partial-->
<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <% if (locals.loggedin) { %>
        <span>Welcome, <%= locals.accountData.account_firstname %></span>
        <a href="/account-management" id="welcome-link">Account Management</a>
        <a href="/logout" id="logout-link">Logout</a>
    <% }else {%>
        <div id="tools">
            <a title="Click to log in" href="/account/login">My Account</a>
        </div>
    <% } %>
    
    
</header>